    "#Load all the packages\n",
    "from sklearn import datasets\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "from sklearn import linear_model"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Data shape: Shape of X = (442, 10)     Shape of Y = (442,)\n"
     ]
    }
   ],
   "source": [
    "#Load the diabetes dataset\n",
    "diabetes = datasets.load_diabetes()\n",
    "Xdata = diabetes.data\n",
    "Ytargets = diabetes.target\n",
    "\n",
    "print('Data shape:', 'Shape of X =', Xdata.shape, '    Shape of Y =', Ytargets.shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Learned Regression Coefficients =  [ -16.57607993 -254.66532396  560.98630022  278.91811152 -393.41357305\n",
      "   97.05460405  -19.0023093   169.46450327  632.95050374  114.21638941]\n",
      "Learned Regression Intercept =  152.347864518\n",
      "Mean squared error (prediction error) =  2794.5690145\n"
     ]
    }
   ],
   "source": [
    "#Linear regression (OLS) using all the 10 variables\n",
    "regr = linear_model.LinearRegression()\n",
    "\n",
    "#Set training data size\n",
    "trainsize = 300\n",
    "\n",
    "#Learn using the first trainsize patient data (each patient data point contains 10 variables/attributes)\n",
    "regr.fit(Xdata[0:trainsize], Ytargets[0:trainsize])\n",
    "\n",
    "#Print the learned coefficients and the intercept\n",
    "print('Learned Regression Coefficients = ', regr.coef_)\n",
    "print('Learned Regression Intercept = ', regr.intercept_)\n",
    "OLS_coeff = regr.coef_\n",
    "\n",
    "#Check the accuracy of the learned predictor on the next 142 patient data\n",
    "print('Mean squared error (prediction error) = ', np.mean((regr.predict(Xdata[trainsize:]) - Ytargets[trainsize:])**2))"
   ]
  },
