   "source": [
    "#Now repeat the above process for Ridge. The stable solutions leads to better predictor error. \n",
    "\n",
    "attr_idx = 2\n",
    "numOfPatients = 3\n",
    "mselist = list()\n",
    "for swin in range(int(442/numOfPatients)):\n",
    "    startId = swin*numOfPatients\n",
    "    endId = (swin+1)*numOfPatients\n",
    "    \n",
    "    regr = linear_model.Ridge(alpha=0.01)\n",
    "    regr.fit(onevardata[startId:endId], Ytargets[startId:endId])\n",
    "    \n",
    "    onevardatA = onevardata[0:startId]\n",
    "    onevardatB = onevardata[endId:]\n",
    "    onevardatCV = np.concatenate((onevardatA, onevardatB), axis=0)\n",
    "    YtargetsA = Ytargets[0:startId]\n",
    "    YtargetsB = Ytargets[endId:]\n",
    "    YtargetsCV = np.concatenate((YtargetsA, YtargetsB), axis=0)\n",
    "    \n",
    "    mselist.append(np.mean((regr.predict(onevardatCV) - YtargetsCV)**2))\n",
    "\n",
    "print(np.mean(mselist))\n",
    "plt.plot(mselist)\n",
    "plt.show()"
   ]
  },
